			"name": "GET /organizations/{organization_id}/optimizations - valid token",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.test(\"Response has expected structure\", function () {",
							"    const jsonData = pm.response.json();",
							"    pm.expect(jsonData).to.be.an('object');",
							"    pm.expect(jsonData).to.have.property(\"optimizations\");",
							"    pm.expect(jsonData.optimizations).to.be.an('object');",
							"    pm.expect(jsonData).to.have.property(\"total_saving\");",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
